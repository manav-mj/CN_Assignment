<div>
  <div>
    <%= Doubt.all.count %><br>
    Doubts Asked
  </div>
  <div>
    <%= Doubt.where(resolved: true).count %><br>
    Doubts Resolved
  </div>
  <div>
    <%= Doubt.where(escalated: true).count %><br>
    Doubts Escalated
  </div>
  <div>
    <%= (Doubt.where(resolved: true).average('resolution_time') || 0) / 60 %>min<br>
    Average Doubts Resolution Time
  </div>

</div>

<ul>
  <%= render User.where(role: 'ta') || User.new %>
</ul>